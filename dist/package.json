{
    "name": "n8n-nodes-toneclone",
    "version": "1.0.2",
    "description": "Write with AI without sounding like AI. Write with AI using your unique voice and style with a model trained on your writing.\n\nTo get started, you'll need a ToneClone API key from https://app.toneclone.ai/api-keys",
    "license": "MIT",
    "homepage": "https://github.com/toneclone/n8n-nodes-toneclone#readme",
    "keywords": [
        "n8n-community-node-package",
        "ai",
        "writing",
        "content-generation",
        "persona",
        "toneclone"
    ],
    "author": {
        "name": "Jon Fox",
        "email": "n8n@toneclone.ai"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/toneclone/n8n-nodes-toneclone.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/TonecloneApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Toneclone/ToneClone.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/jest": "^29.5.12",
        "eslint": "9.32.0",
        "jest": "^29.7.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "ts-jest": "^29.1.2",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
